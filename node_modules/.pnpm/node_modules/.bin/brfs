#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules/.pnpm/brfs@2.0.2/node_modules/brfs/bin/cmd.js/node_modules:/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules/.pnpm/brfs@2.0.2/node_modules/brfs/bin/node_modules:/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules/.pnpm/brfs@2.0.2/node_modules/brfs/node_modules:/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules/.pnpm/brfs@2.0.2/node_modules:/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules/.pnpm/node_modules:/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules:/Users/hyuken/Projects/laravel/node_modules:/Users/hyuken/Projects/node_modules:/Users/hyuken/node_modules:/Users/node_modules:/node_modules:/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules/.pnpm/node_modules/brfs/bin/cmd.js/node_modules:/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules/.pnpm/node_modules/brfs/bin/node_modules:/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules/.pnpm/node_modules/brfs/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../brfs/bin/cmd.js" "$@"
else 
  exec node  "$basedir/../brfs/bin/cmd.js" "$@"
fi
