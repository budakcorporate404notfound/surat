#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules/.pnpm/html-minifier@3.5.21/node_modules/html-minifier/cli.js/node_modules:/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules/.pnpm/html-minifier@3.5.21/node_modules/html-minifier/node_modules:/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules/.pnpm/html-minifier@3.5.21/node_modules:/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules/.pnpm/node_modules:/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules:/Users/hyuken/Projects/laravel/node_modules:/Users/hyuken/Projects/node_modules:/Users/hyuken/node_modules:/Users/node_modules:/node_modules:/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules/.pnpm/html-loader@0.5.5/node_modules/html-loader/node_modules:/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules/.pnpm/html-loader@0.5.5/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../html-minifier@3.5.21/node_modules/html-minifier/cli.js" "$@"
else 
  exec node  "$basedir/../../../../../html-minifier@3.5.21/node_modules/html-minifier/cli.js" "$@"
fi
