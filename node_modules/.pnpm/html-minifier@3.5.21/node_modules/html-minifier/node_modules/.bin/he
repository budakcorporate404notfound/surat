#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules/.pnpm/he@1.2.0/node_modules/he/bin/he/node_modules:/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules/.pnpm/he@1.2.0/node_modules/he/bin/node_modules:/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules/.pnpm/he@1.2.0/node_modules/he/node_modules:/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules/.pnpm/he@1.2.0/node_modules:/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules/.pnpm/node_modules:/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules:/Users/hyuken/Projects/laravel/node_modules:/Users/hyuken/Projects/node_modules:/Users/hyuken/node_modules:/Users/node_modules:/node_modules:/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules/.pnpm/html-minifier@3.5.21/node_modules/html-minifier/node_modules:/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules/.pnpm/html-minifier@3.5.21/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../he@1.2.0/node_modules/he/bin/he" "$@"
else 
  exec node  "$basedir/../../../../../he@1.2.0/node_modules/he/bin/he" "$@"
fi
