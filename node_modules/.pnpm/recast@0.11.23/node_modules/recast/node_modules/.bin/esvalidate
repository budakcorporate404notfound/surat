#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules/.pnpm/esprima@3.1.3/node_modules/esprima/bin/esvalidate.js/node_modules:/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules/.pnpm/esprima@3.1.3/node_modules/esprima/bin/node_modules:/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules/.pnpm/esprima@3.1.3/node_modules/esprima/node_modules:/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules/.pnpm/esprima@3.1.3/node_modules:/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules/.pnpm/node_modules:/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules:/Users/hyuken/Projects/laravel/node_modules:/Users/hyuken/Projects/node_modules:/Users/hyuken/node_modules:/Users/node_modules:/node_modules:/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules/.pnpm/recast@0.11.23/node_modules/recast/node_modules:/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules/.pnpm/recast@0.11.23/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../esprima@3.1.3/node_modules/esprima/bin/esvalidate.js" "$@"
else 
  exec node  "$basedir/../../../../../esprima@3.1.3/node_modules/esprima/bin/esvalidate.js" "$@"
fi
