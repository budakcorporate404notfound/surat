#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules/.pnpm/escodegen@1.2.0/node_modules/escodegen/bin/escodegen.js/node_modules:/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules/.pnpm/escodegen@1.2.0/node_modules/escodegen/bin/node_modules:/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules/.pnpm/escodegen@1.2.0/node_modules/escodegen/node_modules:/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules/.pnpm/escodegen@1.2.0/node_modules:/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules/.pnpm/node_modules:/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules:/Users/hyuken/Projects/laravel/node_modules:/Users/hyuken/Projects/node_modules:/Users/hyuken/node_modules:/Users/node_modules:/node_modules:/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules/.pnpm/ast-transform@0.0.0/node_modules/ast-transform/node_modules:/Users/hyuken/Projects/laravel/L8-Laravel-Admin-Roles-and-Permissions-master/node_modules/.pnpm/ast-transform@0.0.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../escodegen@1.2.0/node_modules/escodegen/bin/escodegen.js" "$@"
else 
  exec node  "$basedir/../../../../../escodegen@1.2.0/node_modules/escodegen/bin/escodegen.js" "$@"
fi
